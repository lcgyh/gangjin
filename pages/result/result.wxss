/* pages/result/result.wxss */
.container_result{
  width: 100%;
  min-height: 100vh;
  background: #EFEFEF;
}
.container_result .pic{
  width: 100%;
  height: 750rpx;
}
.container_result .pic image{
  width: 100%;
  height: 100%;
}
.container_result .result{
  width: 100%;
  text-align: center;
  display: flex;
  flex-direction: column;
  align-items: center;
  background: #fff;
  margin-top: 28rpx;
  padding-bottom: 20rpx;
}
.container_result .result .icon{
  width: 36rpx;
  height: 30rpx;
  margin-top: 60rpx;
}
.container_result .result .icon image{
  width: 100%;
  height: 100%;
}
.container_result .result .title{
  color: #000;
  font-size: 28rpx;
  margin-top: 20rpx;
}
.container_result .result .num{
  color: #000;
  font-size: 52rpx;
  margin-top: 20rpx;
}
.container_result .tab_b{
  width: 100%;
  height: 120rpx;
  color: #fff;
  text-align: center;
  line-height: 120rpx;
  background: #B83000;
  position: fixed;
  bottom: 0;
  left: 0;
}